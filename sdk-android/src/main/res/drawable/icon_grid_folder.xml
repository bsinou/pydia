<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle"
            android:height="@dimen/grid_icon_size"
            android:width="@dimen/grid_icon_size">
            <solid android:color="@color/iconBackground" />
            <corners android:radius="@dimen/list_icon_corner_radius" />
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_baseline_folder_24"
        android:gravity="center"
        android:top="@dimen/margin"
        android:bottom="@dimen/margin_small"
        android:start="@dimen/margin_medium"
        android:end="@dimen/margin_medium"
        android:right="@dimen/margin_medium"
        android:left="@dimen/margin_medium"
        tools:ignore="UnusedAttribute" />
</layer-list>
