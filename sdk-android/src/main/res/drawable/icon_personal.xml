<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:width="@dimen/grid_ws_image_size"
            android:height="@dimen/grid_ws_image_size"
            android:shape="rectangle">
            <solid android:color="@color/iconBackground" />
            <corners android:radius="@dimen/grid_ws_image_corner_radius" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/grid_ws_icon_margin"
        android:drawable="@drawable/ic_baseline_folder_shared_24"
        android:end="@dimen/grid_ws_icon_margin"
        android:gravity="center"
        android:left="@dimen/grid_ws_icon_margin"
        android:right="@dimen/grid_ws_icon_margin"
        android:start="@dimen/grid_ws_icon_margin"
        android:tint="@color/iconTint"
        android:top="@dimen/grid_ws_icon_margin" />
</layer-list>
