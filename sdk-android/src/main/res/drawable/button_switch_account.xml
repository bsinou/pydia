<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:width="@dimen/list_icon_size"
            android:height="@dimen/list_icon_size"
            android:shape="rectangle">
            <solid android:color="@color/iconTint" />
            <corners android:radius="@dimen/list_icon_corner_radius" />
        </shape>
    </item>
    <item android:top="1dp"
        android:bottom="1dp"
        android:end="1dp"
        android:start="1dp"
        android:left="1dp"
        android:right="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/iconBackground" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_baseline_switch_account_24"
        android:gravity="center"
        android:top="@dimen/margin_small"
        android:bottom="@dimen/margin_small"
        android:tint="@color/iconTint"
        android:start="@dimen/margin_small"
        android:end="@dimen/margin_small"/>


</layer-list>
